{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Installation</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Isopen</th>
                <td>{{ entity.isOpen }}</td>
            </tr>
            <tr>
                <th>Reservationsopen</th>
                <td>{{ entity.reservationsOpen }}</td>
            </tr>
            <tr>
                <th>Carsavailable</th>
                <td>{{ entity.carsAvailable }}</td>
            </tr>
            <tr>
                <th>Fallstart</th>
                <td>{{ entity.fallStart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Thanksgivingstart</th>
                <td>{{ entity.thanksgivingStart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Thanksgivingend</th>
                <td>{{ entity.thanksgivingEnd|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fallend</th>
                <td>{{ entity.fallEnd|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Winterstart</th>
                <td>{{ entity.winterStart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Mlkstart</th>
                <td>{{ entity.mlkStart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Mlkend</th>
                <td>{{ entity.mlkEnd|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Springbreakstart</th>
                <td>{{ entity.springbreakStart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Springbreakend</th>
                <td>{{ entity.springbreakEnd|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Winterend</th>
                <td>{{ entity.winterEnd|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fallback</th>
                <td>{{ entity.fallBack|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Springforward</th>
                <td>{{ entity.springForward|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Dailyopen</th>
                <td>{{ entity.dailyOpen }}</td>
            </tr>
            <tr>
                <th>Dailyclose</th>
                <td>{{ entity.dailyClose }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('installation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('installation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
