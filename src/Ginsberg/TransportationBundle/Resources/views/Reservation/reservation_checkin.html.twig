<div id="reservation_checkin">
    {{ form_start(form, {'attr': {'novalidate': 'novalidate' }}) }}
      {{ form_errors(form) }}
      
      <div style='display: none;'>
        {{ form_widget(form.person) }}
        {{ form_row(form.start) }}
        {{ form_row(form.end) }}
        {{ form_row(form.checkout) }}
        {{ form_row(form.checkin) }}
        {{ form_row(form.vehicle) }}
        {{ form_row(form.seatsRequired) }}
        {{ form_row(form.program) }}
        {{ form_row(form.destination) }}
        {{ form_row(form.destinationText) }}
        {{ form_row(form.created) }}
        {{ form_row(form._token) }}
      </div>
      
      {{ form_widget(form.submit, {'label': 'Checkin' }) }}
      
    {{ form_end(form, {'render_rest': false}) }}

</div>